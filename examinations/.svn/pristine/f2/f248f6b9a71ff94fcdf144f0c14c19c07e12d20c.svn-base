*{
	margin: 0px;
	padding: 0px;
}
#title{
	background: #7CFC00;
	height: 50px;
	
}
#title h3{

	text-align: center;
	padding-top: 10px;
}
#seacher{
	margin-top:15px;
	margin-right: 10px;
	margin-left: 10px;
}



.listfont h4{
	color: #1F1F1F;
	font-size: 15px;
	font-weight: normal;
}
p{
		color: #717171;
	
}
#fanhui{
	font-weight: normal;
}
.mui-table-view-cell:after {
	position: absolute;
	right: 0;
	bottom: 0;
	left: 0px;
	height: 1px;
	content: '';
	-webkit-transform: scaleY(.5);
	transform: scaleY(.5);
	background-color: #c8c7cc
}


.mui-navigate-right:after,
.mui-push-right:after {
	right: 10px;
	content: '\e583'
}
.mui-bar .mui-icon {
	font-size: 17px;
	position: relative;
	z-index: 20;
	padding-top: 16px;
	padding-bottom: 10px
}
a {
	text-decoration: none;
	color: #00C89D
}
a:active {
	color: #00C89D
}
input[type=search] {
	font-size: 16px;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	height: 34px;
	text-align: center;
	border: 0;
	border-radius: 6px;
	background-color: #FFFFFF
}
.mui-title{
	font-weight: bold;
}

.mui-pull-bottom-pocket,
.mui-pull-top-pocket {
	
visibility:visible !important;
	
}
.mui-table-view-chevron .mui-table-view-cell {
	padding-right: 15px;
}