#btn {
	width: 100%;
	height: 44px;
	margin: 30px auto;
	color: #FFFFFF;
	font-size: 18px;
	font-weight: normal;
	
}

#butn{
	position:fixed; 
	bottom:-30px;
	left: 1px;
	right: 1px;
}

.mui-btn {
	display: block;
}
.listfont h4{
	color: #1F1F1F;
	font-size: 15px;
	font-weight: normal;
}
p {
	
	color: #717171
}



#fanhui{
	font-weight: normal;
}
.mui-table-view-cell:after {
	position: absolute;
	right: 0;
	bottom: 0;
	left: 0px;
	height: 1px;
	content: '';
	-webkit-transform: scaleY(.5);
	transform: scaleY(.5);
	background-color: #c8c7cc
}
.mui-bar .mui-icon {
	font-size: 17px;
	position: relative;
	z-index: 20;
	padding-top: 16px;
	padding-bottom: 10px
}
a {
	text-decoration: none;
	color: #2E9134
}
a:active {
	color: #2E9134
}
.mui-title{
	font-weight: bold;
}
