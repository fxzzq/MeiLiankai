.mui-bar .mui-btn-link {
	font-size: 16px;
	line-height: 44px;
	top: 0;
	padding: 0;
	color: #00C89D;
	border: 0
}

.listfont {
	color: #1F1F1F;
	font-size: 15px;
	font-weight: normal;
}

#listfont2 {
	color: #717171;
	font-size: 14px;
}

#textarea {
	border: hidden;
}

.mui-table-view-cell:after {
	position: absolute;
	right: 0;
	bottom: 0;
	left: 0px;
	height: 1px;
	content: '';
	-webkit-transform: scaleY(.5);
	transform: scaleY(.5);
	background-color: #c8c7cc
}

.remark {
	word-wrap: break-word;
	word-break: normal;
}
.mui-bar .mui-icon {
	font-size: 17px;
	position: relative;
	z-index: 20;
	padding-top: 16px;
	padding-bottom: 10px
}
a {
	text-decoration: none;
	color: #2E9134
}
a:active {
	color: #2E9134
}
.mui-title{
	font-weight: bold;
}