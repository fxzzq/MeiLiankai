<!doctype html>
<html>
	<head>
		<meta charset="UTF-8">
		<title></title>
		<meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no" />
		<link href="${basePath}_1/lib/mui/css/mui.min.css" rel="stylesheet" />
		<link rel="stylesheet" href="${basePath}Student/css/ClassPractice.css" />
		<style>
			.mui-pull-bottom-pocket,
			.mui-pull-top-pocket {
				visibility: visible;
			}
		</style>
		<script src="${basePath}_1/lib/mui/js/mui.min.js"></script>
		<script type="text/javascript" src="${basePath}_1/lib/jquery-1.7.2.min.js"></script>
		<script type="text/javascript" src="${basePath}_1/js/Outside.js"></script>
		<script type="text/javascript" src="${basePath}_1/js/StorageKeys.js"></script>
	</head>

	<body>
		<div id="pullrefresh" class="mui-content mui-scroll-wrapper">
			<div class="mui-scroll">
				<div class="mui-input-row mui-search" id="seacher">
					<input type="search" class="mui-input-clear" placeholder="搜索">
				</div>
				<ul class="mui-table-view mui-table-view-striped mui-table-view-condensed">
				
				</ul>
			</div>
		</div>
<script type="text/javascript">
			mui.init();
		   /**
			 * 跳转至随堂练习详情页面
			 */
			mui('.mui-table-view').on('tap', '.mui-table-view-cell', function() {
				var id = $(this).attr("id");
				var url = 'Answer.html';
				if($(this).hasClass("success")){
					url = 'AnswerResult.html';
				}
				storage.setItem($LOCALKAY_STUDENT_CLASSPRACTICE_UUID,id);
				mui.openWindow({
					url: url
				});
			});
			 
		var start = cstart;
		var  value="";
		var pulldownstart=cstart;
		var pullupstart=cstart;
		
		$("#input").keydown(function(e){		
  		 if(e.keyCode == 13){ 
  		 start = cstart;
  		 }
		});
		      /**
			 * 页面数据加载   自动加载一次
			 */
			  $(function(){
  			$.ajax("${basePath}proj/examinations/practicelist.action",{
					type:"get",
					async:false,
					dataType:"json",
					data:{"start":cstart,"limit":overall_limit_size,"keyword":value},
					success:function(resp){
						var data = resp.data;
						if(resp && resp.success && data){
						start+=data.length;
							$(".mui-table-view").empty();
							var html = "";
							$.each(data,function(i,v){
							html+=("<li class='mui-table-view-cell "+(v.isfinish==true?"success":"error")+"'id='"+v.id+"'>"+
						           "<div class='mui-slider-handle mui-table'>"+
							   		"<div class='mui-table-cell mui-col-xs-10 listfont'>"+
								    "<h4 class='mui-ellipsis'>"+v.name+"</h4>"+
									"<p class='mui-ellipsis listfont2'>"+v.type+"</p></div>"+
							        "<div class='mui-table-cell mui-col-xs-2 mui-text-right'>"+
								    "<span class='mui-h5 style='"+(v.isfinish==true?"color:#4cd964":"color:red")+"'>"+(v.isfinish==true?"已完成":"未完成")+"</span></div></div></li>");
							});
							$(".mui-table-view").append(html);
						}
						else{
							if(resp.emsg){
								mui.toast(resp.emsg);
							}
						}
					},
				    error:function(){
				    	
				    }
				});
		
			});
		          

		   /**
			 * 下拉刷新 上拉加载
			 */
			mui.init({
				pullRefresh: {
					container: '#pullrefresh',
					down: {
						contentdown: "下拉可以刷新", 
						contentover: "释放立即刷新", 
						contentrefresh: "正在刷新...",
						//auto:true,
						callback: pulldownRefresh
					},
					up: {
					height: 100, //距离底部高度(到达该高度即触发)
				
					contentinit: '上拉显示更多',
					contentdown: '上拉显示更多',
					contentrefresh: '正在加载...',
					contentnomore: '没有更多数据了',
					callback: pullupRefresh
					}
				}
			});
			
			/**
			 * 下拉刷新具体业务实现
			 */
			function pulldownRefresh() {
			mui('#pullrefresh').pullRefresh().refresh(true);
				setTimeout(function() {
				value=document.getElementById("input").value;
				$.ajax("${basePath}proj/examinations/practicelist.action",{
					type:"get",
					async:false,
					dataType:"json",
					data:{"start":cstart,"limit":overall_limit_size,"keyword":value},
					success:function(resp){
						var data = resp.data;
						
						if(resp && resp.success && data){
						start=data.length;
						
							$(".mui-table-view").empty();
							var html = "";
							$.each(data,function(i,v){
							html+=("<li class='mui-table-view-cell "+(v.isfinish==true?"success":"error")+"'id='"+v.id+"'>"+
						           "<div class='mui-slider-handle mui-table'>"+
							   		"<div class='mui-table-cell mui-col-xs-10 listfont'>"+
								    "<h4 class='mui-ellipsis'>"+v.name+"</h4>"+
									"<p class='mui-ellipsis listfont2'>"+v.type+"</p></div>"+
							        "<div class='mui-table-cell mui-col-xs-2 mui-text-right'>"+
								    "<span class='mui-h5 style='"+(v.isfinish==true?"color:#4cd964":"color:red")+"'>"+(v.isfinish==true?"已完成":"未完成")+"</span></div></div></li>");
							  
							});
							$(".mui-table-view").append(html);
						}
						else{
							if(resp.emsg){
								mui.toast(resp.emsg);
							}
						}
						
		
					},
				    error:function(){
				    	
				    }
				});
	      
					mui('#pullrefresh').pullRefresh().endPulldownToRefresh();
					mui('#pullrefresh').pullRefresh().endPullupToRefresh(false);
				}, 1500);
			}
			var count = 0;
			/**
			 * 上拉加载具体业务实现
			 */
			
			function pullupRefresh() {	
				setTimeout(function() {		
				$.ajax("${basePath}proj/examinations/practicelist.action",{
					type:"get",
					async:false,
					dataType:"json",
					data:{"start":start,"limit":overall_limit_size,"keyword":value},
					success:function(resp){
						var data = resp.data;
						if(resp && resp.success && data){
						start+=data.length;
						pullupstart=start;
							var html = "";
							$.each(data,function(i,v){
							html+=("<li class='mui-table-view-cell "+(v.isfinish==true?"success":"error")+"'id='"+v.id+"'>"+
						           "<div class='mui-slider-handle mui-table'>"+
							   		"<div class='mui-table-cell mui-col-xs-10 listfont'>"+
								    "<h4 class='mui-ellipsis'>"+v.name+"</h4>"+
									"<p class='mui-ellipsis listfont2'>"+v.type+"</p></div>"+
							        "<div class='mui-table-cell mui-col-xs-2 mui-text-right'>"+
								    "<span class='mui-h5 style='"+(v.isfinish==true?"color:#4cd964":"color:red")+"'>"+(v.isfinish==true?"已完成":"未完成")+"</span></div></div></li>");
							});
							$(".mui-table-view").append(html);
							mui('#pullrefresh').pullRefresh().endPullupToRefresh(((data.length<overall_limit_size)||data.length==0)	);		
															
						}
						else{
							if(resp.emsg){
								mui.toast(resp.emsg);
							}
						}
					},
				    error:function(){				    	
				    }
				});
				}, 1500);
				
			}
			
		   /**
			 * 随堂练习 搜索
			 */		
		$("#input").keydown(function(e){		
  		 if(e.keyCode == 13){   		 
  		  value=document.getElementById("input").value;  		
  			$.ajax("${basePath}proj/examinations/practicelist.action",{
					type:"get",
					async:false,
					dataType:"json",
					data:{"start":cstart,"limit":overall_limit_size,"keyword":value},
					success:function(resp){
						var data = resp.data;
						if(resp && resp.success && data){
						//alert("回车前"+start);
						start+=data.length;
						//alert("回车后"+start);
						//start=cstart;
							$(".mui-table-view").empty();
							var html = "";
							$.each(data,function(i,v){
						html+=("<li class='mui-table-view-cell "+(v.isfinish==true?"success":"error")+"'id='"+v.id+"'>"+
						           "<div class='mui-slider-handle mui-table'>"+
							   		"<div class='mui-table-cell mui-col-xs-10 listfont'>"+
								    "<h4 class='mui-ellipsis'>"+v.name+"</h4>"+
									"<p class='mui-ellipsis listfont2'>"+v.type+"</p></div>"+
							        "<div class='mui-table-cell mui-col-xs-2 mui-text-right'>"+
								    "<span class='mui-h5 style='"+(v.isfinish==true?"color:#4cd964":"color:red")+"'>"+(v.isfinish==true?"已完成":"未完成")+"</span></div></div></li>");							  
							});
							$(".mui-table-view").append(html);
						}
						else{
							if(resp.emsg){
								mui.toast(resp.emsg);
							}
						}
					},
				    error:function(){				    	
				    }
				});
  		} 		
  		});
		</script>
	</body>

</html>