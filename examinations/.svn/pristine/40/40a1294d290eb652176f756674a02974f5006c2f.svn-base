.inputset {
	margin-top: 10px;
}

.inputset label {}

.inputset input {
	width: 80%;
}

.mui-table-view-cell:after {
	position: absolute;
	right: 0;
	bottom: 0;
	left: 0px;
	height: 1px;
	content: '';
	-webkit-transform: scaleY(.5);
	transform: scaleY(.5);
	background-color: #c8c7cc
}

.mui-table-view:after {
	position: absolute;
	right: 0;
	bottom: 0;
	left: 0;
	height: 0px;
	content: '';
	-webkit-transform: scaleY(.5);
	transform: scaleY(.5);
	background-color: #c8c7cc
}

.formset {
	width: 180px;
	float: right;
	margin-right: 50px;
	text-align: right;
}
.mui-bar .mui-icon {
	font-size: 17px;
	position: relative;
	z-index: 20;
	padding-top: 16px;
	padding-bottom: 10px
}
a {
	text-decoration: none;
	color: #00C89D
}
a:active {
	color: #00C89D
}
.mui-title{
	font-weight: bold;
}
