<!doctype html>
<html>

	<head>
		<meta charset="UTF-8">
		<title></title>
		<meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no" />
		<link href="${basePath}_1/lib/mui/css/mui.min.css" rel="stylesheet" />
		<link rel="stylesheet" href="${basePath}Teacher/css/Task.css" />
		
	</head>

	<body>
		<header class="mui-bar mui-bar-nav">
			<button type="button" class="mui-left mui-action-back mui-btn  mui-btn-link mui-btn-nav mui-pull-left">
					<span class="mui-icon mui-icon-left-nav"></span>返回
				</button>
			<h1 class="mui-title">作业</h1>
		</header>
		<div class="mui-content">
			<div class="butn mui-content-padded" id="butn">
				<button type="button" id="btn" class="meloclan-mui-btn mui-btn">布置作业</button>
			</div>
		</div>
		

		<script src="${basePath}_1/lib/mui/js/mui.min.js"></script>
		<script type="text/javascript">
		document.getElementById("fanhui").addEventListener("tap",function(){
			mui.openWindow({
				url: 'index.html'
			});
		});
			//启用双击监听
		mui.init({
			gestureConfig: {
				doubletap: true
			},
			subpages: [{
				url: 'Task_sub.html',
				id: 'Task_sub',
				styles: {
					top: '45px',
					bottom: '68px',
				}
			}]
		});

		var contentWebview = null;
		document.querySelector('header').addEventListener('doubletap', function() {
			if(contentWebview == null) {
				contentWebview = plus.webview.currentWebview().children()[0];
			}
			contentWebview.evalJS("mui('#pullrefresh').pullRefresh().scrollTo(0,0,100)");
		});
			mui('#butn').on('tap', '#btn', function() {
				mui.openWindow({
					url: 'ArrangeTask2.html'
				});
			})
		</script>
		
	</body>

</html>