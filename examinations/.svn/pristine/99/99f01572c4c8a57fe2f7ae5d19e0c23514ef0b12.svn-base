#title{
	background: #FFFFFF;
}
.tablestyle{
	width: 100%;
}

#xzq {
	font: "微软雅黑";
	font-size: 18px;
	color: #1F1F1F;
	text-align: center;
	font-weight: normal;
	margin-top: 1px;
	padding-top: 30px;
	padding-bottom: 20px;
}

#fanhui {
	font-weight: normal;
}

body {
	background: #FFFFFF;
}

.mui-bar .mui-icon {
	font-size: 17px;
	position: relative;
	z-index: 20;
	padding-top: 16px;
	padding-bottom: 10px
}

a {
	text-decoration: none;
	color: #00C89D
}

a:active {
	color: #00C89D
}

.mui-title {
	font-weight: bold;
}

table {
	border-collapse: collapse;
	border-spacing: 0px;
	width: 100%;
	border: #000 solid 0px;
}

table td {
	border: 1px solid #000;
	height: 33px;
	text-align: center;
	border-left: 0px;
	
}

table th {
	background: #00C89D;
	color: #fff;
	border: #000 solid 1px;
	white-space: nowrap;
	height: 37px;
	border-top: 0px;
	border-left: 0px;
	
}
table tr {
	padding:0px 5px;
}
.rightnameTP th{
    padding:0px 10px;	
}

.t_left {
	width: 20%;
	float: left;
	border-top: 1px solid #000;
	border-left: 1px solid #000;
}

.t_r_content {
	width: 100%;
	height: 100%;
	background: #fff;
	overflow: auto;
}

t_r_content table td{
	width: 40px;
}

.cl_freeze {
	
	height: 100%;
	overflow: hidden;
	width: 100%;
}

.t_r {
	width: 80%;
	height: auto;
	float: left;
	border-top: 1px solid #000;
	border-right: #000 solid 1px;
}

t_r_title table th{
	width: 40px;
}

.t_r_t {
	width: 100%;
	overflow: hidden;
}

.bordertop {
	border-top: 0px;
}