
.listfont h4{
	color: #1F1F1F;
	font-size: 15px;
	font-weight: normal;
}
.listfont2{
		color: #717171;
	font-size: 14px;
}
.mui-bar .mui-btn-link {
	font-size: 16px;
	line-height: 44px;
	top: 0;
	padding: 0;
	color: #00C89D;
	border: 0
}
.mui-table-view-cell:after {
	position: absolute;
	right: 0;
	bottom: 0;
	left: 0px;
	height: 1px;
	content: '';
	-webkit-transform: scaleY(.5);
	transform: scaleY(.5);
	background-color: #c8c7cc
}

#seacher{
	margin-top:15px;
	margin-right: 10px;
	margin-left: 10px;
}
.mui-bar .mui-icon {
	font-size: 17px;
	position: relative;
	z-index: 20;
	padding-top: 16px;
	padding-bottom: 10px
}
a {
	text-decoration: none;
	color: #00C89D
}
a:active {
	color: #00C89D
}
input[type=search] {
	font-size: 16px;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	height: 34px;
	text-align: center;
	border: 0;
	border-radius: 6px;
	background-color: #FFFFFF
}
.mui-title{
	font-weight: bold;
}